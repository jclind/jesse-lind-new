---
import DefaultFileList from '../../components/Common/DefaultFileList';
import type { NavLinkType } from '../../components/Common/NavHeader/NavHeader';
import Layout from '../../layouts/Layout.astro';
import '../../styles/global.scss';

const navLinks = [{name: 'jesselind', src:'/'}, {name: 'files', src:'/files'}, {name:'legal', src:'/files/legal'}]

const fileLinks = [
  {name: 'privacy_policy', src: '/files/legal/privacy-policy', type: '.txt'},
  {name: 'terms_of_service', src:'/files/legal/terms-of-service', type: '.txt'}
]
---

<Layout title="legal/">
  <DefaultFileList navLinks={navLinks} fileLinks={fileLinks} client:load />
</Layout>
